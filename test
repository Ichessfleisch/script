local Webhook_URL = 'https://discord.com/api/webhooks/1238574016130973697/dafOijZOxJrgyeBdvu_qRDCziZAjg2nQQ-4XG5ed1_UE2_cerYggRqKfvZ6X5nLmmKnv'

-- Function to generate a 19-digit random number
local function generate19DigitNumber()
    local num = ""
    for i = 1, 19 do
        num = num .. tostring(math.random(0, 9))
    end
    return num
end

-- Create an Embed with various options
local embed = {
    ["title"] = "New Execution",
    ["description"] = "This is a new execution with a 19-digit random number: " .. generate19DigitNumber(),
    ["color"] = 65280, -- Green
    ["footer"] = {
        ["text"] = "hi"
    },
    ["timestamp"] = os.date("!%Y-%m-%dT%H:%M:%S", os.time()), -- Current timestamp in GMT timezone
    ["image"] = {
        ["url"] = "https://example.com/image.png"
    }
}

-- Create the data for the webhook message
local data = {
    content = "New Log!",
    embeds = {embed}
}

-- JSON encode the data
local jsonData = game:GetService("HttpService"):JSONEncode(data)

-- Send the data to the Discord webhook
request({
    Url = Webhook_URL,
    Method = "POST",
    Headers = {
        ["Content-Type"] = "application/json"
    },
    Body = jsonData
})
