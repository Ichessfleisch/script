local Webhook_URL = 'https://discord.com/api/webhooks/1238574016130973697/dafOijZOxJrgyeBdvu_qRDCziZAjg2nQQ-4XG5ed1_UE2_cerYggRqKfvZ6X5nLmmKnv'

-- Erstelle ein Embed mit verschiedenen Optionen
local embed = {
    ["title"] = "New Execution",
    ["description"] = "This is a new execution.",
    ["color"] = 65280, -- Grün
    ["footer"] = {
        ["text"] = "hi"
    },
    ["timestamp"] = os.date("!%Y-%m-%dT%H:%M:%S", os.time()), -- Aktueller Zeitstempel in GMT-Zeitzone
    ["image"] = {
        ["url"] = "https://example.com/image.png"
    }
}

-- Erstelle die Daten für die Webhook-Nachricht
local data = {
    content = "New Log!",
    embeds = {embed}
}

-- JSON-kodiere die Daten
local jsonData = game:GetService("HttpService"):JSONEncode(data)

-- Sende die Daten an den Discord-Webhook
request({
    Url = Webhook_URL,
    Method = "POST",
    Headers = {
        ["Content-Type"] = "application/json"
    },
    Body = jsonData
})
